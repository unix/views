datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model View {
  id Int @id @default(autoincrement())
  name String @unique
  host String?
  createdAt DateTime @default(now())
  pages Page[]
}

model Page {
  id Int @id @default(autoincrement())
  name String @unique
  page String?
  count Int
  view_id Int
  view View @relation(fields: [view_id], references: [id])
}

